		"@prisma/client": "^3.5.0",
		"apollo-server": "^3.5.0",
		"bcrypt": "^5.0.1",
		"bcryptjs": "~2.4.0",
		"dotenv": "^16.0.0",
		"graphql": "^15.7.2",
		"graphql-scalars": "^1.14.1",
